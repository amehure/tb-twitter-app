<p id="notice"><%= notice %></p>

<h1>Ddays</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ddays.each do |dday| %>
      <tr>
        <td><%= link_to 'Show', dday %></td>
        <td><%= link_to 'Edit', edit_dday_path(dday) %></td>
        <td><%= link_to 'Destroy', dday, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dday', new_dday_path %>
